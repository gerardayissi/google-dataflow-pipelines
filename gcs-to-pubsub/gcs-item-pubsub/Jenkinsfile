pipeline {
   agent any
       tools {
           maven 'Maven 3.6.3'
           jdk 'jdk8'
       }
   stages {
     stage ('Build') {
      node {
        git url: 'https://github.com/gerardayissi/google-dataflow-pipelines.git'
        withMaven (
        maven: 'Maven 3.6.3',
        jdk:   'jdk8'
        ){
          //sh "mvn clean verify"
          sh "mvn install"
        }
      }
    }
}
}