pipeline {
   agent any
       tools {
           maven 'Maven 3.6.3'
           jdk 'jdk8'
       }
   stages {
   stage('Build') {
       steps {
           sh 'echo "Hello World"'
           sh '''
               echo "Multiline shell steps works too"
               ls -lah
           '''
       }
   }
     stage ('Compile Maven Project') {
      steps {
        git url: 'https://github.com/gerardayissi/google-dataflow-pipelines.git'
        withMaven (
        maven: 'Maven 3.6.3',
        jdk:   'jdk8'
        ){
          //sh "mvn clean verify"
          sh "mvn install"
        }
      }
    }
}
}